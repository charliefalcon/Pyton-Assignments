   "source": [
    "year = int(input('Enter year to check:'))\n",
    "if year % 4 == 0 :\n",
    "    leap = 'True'\n",
    "elif year % 100 == 0 and year % 400 == 0:\n",
    "    leap = 'True'\n",
    "else:\n",
    "    leap = 'False'\n",
    "\n",
    "if leap == 'True':\n",
    "    print(year, \"is a leap year\")\n",
    "else:\n",
    "     print(year, \"is not a leap year\")"
